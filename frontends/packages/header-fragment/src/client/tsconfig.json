{
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "es6",
    "module": "commonjs",
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "declaration": true,
    "strictPropertyInitialization": true,
    "sourceMap": true,
    "outDir": "../../build/client",
    "jsx": "react",
    "noImplicitAny": true
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "build"
  ]
}
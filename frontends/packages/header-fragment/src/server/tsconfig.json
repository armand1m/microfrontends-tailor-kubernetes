{
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "es2017",
    "module": "commonjs",
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "declaration": true,
    "strictPropertyInitialization": true,
    "sourceMap": true,
    "outDir": "../../build/server",
    "jsx": "react",
    "noImplicitAny": true
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "build",
    "../../node_modules"
  ]
}